#content{
	height: 12rem;
	background: url("../img/ce-bg.png") no-repeat;
    -webkit-background-size: 100%;
    background-size: 100% 100%;
    margin-bottom: 1.1rem;
    overflow-y: scroll;
}
.gen-one{
	width: 96%;
	min-height: 0rem;	
	margin: 0 auto;
	box-sizing: border-box;
	
}
.gen-box{
	float: left;
	width: 20%;
	height: 1.45rem;
	border: 1px solid #96d9ff;
	border-radius:50%;
	margin-top: 0.2rem;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	margin-top: 0.1rem;
	margin-left: 2%;
	margin-right: 3%;
	margin-bottom: 0.2rem;
	color: #FFFFFF;
}
.gen-box-xian{
	width: 96%;
	height: 20%;
		font-size: 0.2rem;
		text-align: center;
}
.chengwei{
	width: 96%;
	
	/*border: 1px solid #000000;*/
	border-radius: 0.1rem;
	
	text-align: center;
	
	overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;
}
.xuanzhe{
	height: 100%;
	display: flex;
	justify-content: space-around;
	align-items: center;	
	font-size: 30px;		
}
.inp{
	 transform:scale(2);
	 margin-right: 10px;
}