html,
body {
	width: 100%;
	height: 100%;
	background-color: #FFFFFF;
}

.fabu {
	width: 100%;
}

.fabu span {
	width: 30%;
}

.fabu-title {
	width: 100%;
	display: flex;
	justify-content: flex-start;
	align-items: center;
	border-bottom: 10px solid #EEEEEE;
	font-size: 16px;
	height: 70px;
	padding: 10px;
	box-sizing: border-box;
}

.fabu-title>input {
	height: 40px;
	border: none;
}

.fabu-time {
	width: 100%;
	display: flex;
	flex-direction: column;
	justify-content: flex-start;
	align-items: flex-start;
	border-bottom: 10px solid #EEEEEE;
	font-size: 16px;
	padding: 10px;
	box-sizing: border-box;
}

.relation-list {
	width: 100%;
	display: flex;
	flex-direction: column;
	justify-content: flex-start;
	align-items: flex-start;
	
	padding: 0.35rem;
	box-sizing: border-box;
}
.relation-one{
	width: 100%;
	margin-bottom: 0.2rem;
}
.relation-two{
	width: 100%;
	display: flex;
	justify-content: flex-start;
	align-items:flex-start;
	font-size: 0.26rem;
	color: #000000;
	margin-bottom: 0.1rem;
}
.relation-two>span{
	width: 20%;
}
.relation-two>div{
	width: 80%;
    /*word-wrap: break-word;
    word-break: normal;*/
     word-break:break-all;
     word-wrap:break-word;
}
.roywl{
	width: 1.75rem;
    height: 0.6rem;
    background-color: #4C4A8E;
    color: #FFFFFF;
    font-size: 0.3rem;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 0.3rem;
    
}
.roywl-box{
	width: 100%;
	display: flex;
	justify-content: space-around;
	align-items: center;
	margin-top: 0.2rem;
}
.fabu-content {
	width: 100%;
	height: 1.55rem;
	background-color: #4C4A8E;
	display: flex;
	justify-content:center;
	align-items: center;
	
}
.tips{
	height: 2rem;
	width: 96%;
	border-radius: 0.2rem;
	margin: 1.3rem auto 0.2rem;
	background-color: rgba(225, 222, 255, 0.54);
    color: #4C4A8E;
    filter: Alpha(opacity=54);
	font-size:0.3rem ;
	display: flex;
	justify-content: center;
	align-items: center;
}
.chaxun{
	width: 1.85rem;
    height: 0.7rem;
    border-radius: 0.33rem;
    box-sizing: border-box;
    background-color: #4C4A8E;
    border: 2px solid #b4b4dc;
    text-align: center;
    line-height: 0.66rem;
    color: #FFFFFF;
    font-size: 0.26rem;
}
