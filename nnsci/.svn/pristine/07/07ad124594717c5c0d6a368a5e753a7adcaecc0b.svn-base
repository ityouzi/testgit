rver&quot;</p><p>set SVN_ROOT=E:\\Repositories</p><p>set SVN_BACKUP_ROOT=F:\\bak</p><p>set TIME_DIR=%date:~,4%%date:~5,2%%date:~8,2%_%time:~,2%%time:~3,2%%time:~6,2%%time:~9,2%</p><p>set BACKUP_DIRECTORY=%SVN_BACKUP_ROOT%\\%TIME_DIR%</p><p>set LOG=%BACKUP_DIRECTORY%\\backup.log</p><p>set NUM=30</p><p>if not exist %SVN_HOME% goto error</p><p>goto start</p><p><br/></p><p>:start</p><p><br/></p><p>mkdir %BACKUP_DIRECTORY%</p><p>@echo [info]%date:~,10% %time:~,2%:%time:~3,2%:%time:~6,2%建立备份目录：%BACKUP_DIRECTORY% &gt;&gt;%LOG%</p><p>for /d %%i in (%SVN_ROOT%\\*) do (</p><p>%SVN_HOME%\\bin\\svnadmin dump %SVN_ROOT%\\%%~ni &gt;%BACKUP_DIRECTORY%\\%%~ni.dmp 2&gt;&gt;%LOG%</p><p>)</p><p>@echo [info]%date:~,10% %time:~,2%:%time:~3,2%:%time:~6,2%退出码为：%errorlevel%&gt;&gt;%LOG%</p><p>at 23:00 /next:S %~f0&gt;&gt;%LOG%</p><p><br/></p><p>goto delete</p><p>&nbsp;</p><p>:delete</p><p><br/></p><p>@echo [info]%date:~,10% %time:~,2%:%time:~3,2%:%time:~6,2%删除%NUM%天前的备份:&gt;&gt;%LOG%</p><p>forfiles /P %SVN_BACKUP_ROOT% /M *_* /D -%NUM% /C &quot;cmd /c echo 删除文件夹：@file&quot;&gt;&gt;%LOG%</p><p>forfiles /P %SVN_BACKUP_ROOT% /M *_* /D -%NUM% /C &quot;cmd /c rd /s/q @path&quot; 2&gt;&gt;%LOG%</p><p>goto end</p><p>&nbsp;</p><p>:error</p><p>echo [error]%date:~,10% %time:~,2%:%time:~3,2%:%time:~6,2%程序目录不存在，请检查：%SVN_HOME%&gt;&gt;%LOG%</p><p><br/></p><p>goto end</p><p>&nbsp;</p><p>:end</p><p><br/></p><p>@echo. &gt;&gt;%LOG%</p><p>PAUSE</p><p><br/></p><p>脚本详细解答：</p><p>http://www.cnblogs.com/yadongliang/p/9297058.html</p>',NULL,NULL,NULL,NULL),('dfe55150787a4ba3be5dc3a1cc536946','0966864c1c5a4981bdd54d23191fe300','admin','2018-08-30 10:58:51','<p><img src=\"/nnsci/plugins/ueditor/jsp/upload1/20180830/40441535597930799.png\"/></p>',NULL,NULL,NULL,NULL),('e6172d0385b146b6bb24a9a1ea957d47','4fe121cbf1d643509ab9f64abaaf9637','admin','2018-12-04 20:08:15','<p>测试。。。。</p>',NULL,NULL,NULL,NULL);

