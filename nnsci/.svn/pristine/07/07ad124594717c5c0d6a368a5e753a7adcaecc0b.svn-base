/*Table structure for table `tb_pictures` */

DROP TABLE IF EXISTS `tb_pictures`;

CREATE TABLE `tb_pictures` (
  `PICTURES_ID` varchar(100) NOT NULL,
  `TITLE` varchar(255) default NULL COMMENT '标题',
  `NAME` varchar(255) default NULL COMMENT '文件名',
  `PATH` varchar(255) default NULL COMMENT '路径',
  `CREATETIME` varchar(100) default NULL COMMENT '创建时间',
  `MASTER_ID` varchar(255) default NULL COMMENT '属于',
  `BZ` varchar(255) default NULL COMMENT '备注',
  PRIMARY KEY  (`PICTURES_ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

/*Data for the table `tb_pictures` */

/*Table structure for table `tb_question` */

DROP TABLE IF EXISTS `tb_question`;

CREATE TABLE `tb_question` (
  `QUESTION_ID` varchar(100) NOT NULL,
  `CREATOR` varchar(255) default NULL COMMENT '创建人',
  `CREATE_TIME` varchar(32) default NULL COMMENT '创建时间',
  `STATUS` varchar(255) default NULL COMMENT '状态',
  `QUESTION` longtext COMMENT '问题',
  `TYPE` varchar(255) default NULL COMMENT '类型',
  `YL1` varchar(255) default NULL COMMENT '预留1',
  `YL2` varchar(255) default NULL COMMENT '预留2',
  `YL3` varchar(255) default NULL COMMENT '预留3',
  `YL4` varchar(255) default NULL COMMENT '预留4',
  `YL5` varchar(255) default NULL COMMENT '预留5',
  `YL6` varchar(255) default NULL COMMENT '预留6',
  `YL7` varchar(255) default NULL COMMENT '预留7',
  `YL8` varchar(255) default NULL COMMENT '预留8',
  `YL9` varchar(255) default NULL COMMENT '预留9',
  `YL10` varchar(255) default NULL COMMENT '预留10',
  PRIMARY KEY  (`QUESTION_ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

