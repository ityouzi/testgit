<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="EtermsmallMapper">
	
	<!--表名 -->
	<sql id="tableName">
		TB_ETERMSMALL
	</sql>
	
	<!-- 字段 -->
	<sql id="Field">
		CREATE_BY,	
		CREATE_TIME,	
		STATUS,	
		USERID,	
		ACCOUNT,	
		PASSWORD,	
		REMARK,	
		YL1,	
		YL2,	
		YL3,	
		YL4,	
		YL5,	
		YL6,	
		YL7,	
		YL8,	
		YL9,	
		YL10,	
		YL11,	
		YL12,	
		YL13,	
		YL14,	
		YL15,	
		YL16,	
		YL17,	
		YL18,	
		YL19,	
		YL20,	
		ETERMSMALL_ID
	</sql>
	
	<!-- 字段值 -->
	<sql id="FieldValue">
		#{CREATE_BY},	
		#{CREATE_TIME},	
		#{STATUS},	
		#{USERID},	
		#{ACCOUNT},	
		#{PASSWORD},	
		#{REMARK},	
		#{YL1},	
		#{YL2},	
		#{YL3},	
		#{YL4},	
		#{YL5},	
		#{YL6},	
		#{YL7},	
		#{YL8},	
		#{YL9},	
		#{YL10},	
		#{YL11},	
		#{YL12},	
		#{YL13},	
		#{YL14},	
		#{YL15},	
		#{YL16},	
		#{YL17},	
		#{YL18},	
		#{YL19},	
		#{YL20},	
		#{ETERMSMALL_ID}
	</sql>
	
	<!-- 新增-->
	<insert id="save" parameterType="pd">
		insert into 
	<include refid="tableName"></include>
		(
	<include refid="Field"></include>
		) values (
	<include refid="FieldValue"></include>
		)
	</insert>
	
	<!-- 删除-->
	<delete id="delete" parameterType="pd">
		delete from
		<include refid="tableName"></include>
		where 
			ETERMSMALL_ID = #{ETERMSMALL_ID}
	</delete>
	
	<!-- 修改 -->
	<update id="edit" parameterType="pd">
		update
		<include refid="tableName"></include>
		set 
			CREATE_BY = #{CREATE_BY},
			CREATE_TIME = #{CREATE_TIME},
			STATUS = #{STATUS},
			USERID = #{USERID},
			ACCOUNT = #{ACCOUNT},
			PASSWORD = #{PASSWORD},
			REMARK = #{REMARK},
			YL1 = #{YL1},
			YL2 = #{YL2},
			YL3 = #{YL3},
			YL4 = #{YL4},
			YL5 = #{YL5},
			YL6 = #{YL6},
			YL7 = #{YL7},
			YL8 = #{YL8},
			YL9 = #{YL9},
			YL10 = #{YL10},
			YL11 = #{YL11},
			YL12 = #{YL12},
			YL13 = #{YL13},
			YL14 = #{YL14},
			YL15 = #{YL15},
			YL16 = #{YL16},
			YL17 = #{YL17},
			YL18 = #{YL18},
			YL19 = #{YL19},
			YL20 = #{YL20},
		ETERMSMALL_ID = ETERMSMALL_ID
		where 
		ETERMSMALL_ID = #{ETERMSMALL_ID}
	</update>
	
	<!-- 通过ID获取数据 -->
	<select id="findById" parameterType="pd" resultType="pd">
		select 
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
		where 
			ETERMSMALL_ID = #{ETERMSMALL_ID}
	</select>
	
	<!-- 列表 -->
	<select id="datalistPage" parameterType="page" resultType="pd">
		select
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
		where 1=1
		<if test="pd.keywords!= null and pd.keywords != ''"><!-- 关键词检索 -->
			and
				(
					CREATE_BY LIKE CONCAT(CONCAT('%',#{pd.keywords}),'%')
				)
		</if>
		<if test="pd.CreateBy!=null">						<!-- 用户查看权限 -->
			and CREATE_BY = #{pd.CreateBy}
		</if>
		<if test="pd.name!=null and pd.name!=''"> 			<!-- 状态检索 -->
			and STATUS = #{pd.name}
		</if>
		<!-- <if test="pd.lastStart!=null and pd.lastStart!=''"> 开始时间检索
			and TIMELIMIT &gt;= #{pd.lastStart}
		</if>
		<if test="pd.lastEnd!=null and pd.lastEnd!=''">		结束时间检索
			and TIMELIMIT &lt;= #{pd.lastEnd}
		</if> -->
		order by CREATE_TIME desc							<!-- 降序 -->
	</select>
	
	<!-- 列表(全部) -->
	<select id="listAll" parameterType="pd" resultType="pd">
		select
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
	</select>
	
	<!-- 批量删除 -->
	<delete id="deleteAll" parameterType="String">
		delete from
		<include refid="tableName"></include>
		where 
			ETERMSMALL_ID in
		<foreach item="item" index="index" collection="array" open="(" separator="," close=")">
                 #{item}
		</foreach>
	</delete>
	
	<!-- fh313596790qq(青苔) -->
</mapper>