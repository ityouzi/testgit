		<script>
//			移动端底部栏点击跳转页面
			mui('.dibu-box').on('tap','a',function(){document.location.href=this.href;});
			mui('header').on('tap','a',function(){document.location.href=this.href;});
            var carche;
            var jieche;
            var orderInfo;
            var orderid = GetRequest().orderid;
           
             
            //var roletype= loginInfo.userInfo.roletype;
		$(function(){
//				点击折叠切换
				$('.od-top').each(function(i){
					$(this).on('tap',function(){
						if($('.wrap').eq(i).css('display')=='none'){
							$('.wrap').eq(i).css('display','block');
							$(this).css('background-color','#F4F3F8');
							$('.od-top>img').eq(i).attr("src","../my-img/Order-details/xia@1.png");
						}else{
							$('.wrap').eq(i).css('display','none');
							$(this).css('background-color','#ffffff');
							$('.od-top>img').eq(i).attr("src","../my-img/Order-details/shang@1.png");
						}
					})
				})
				
//		订单详情渲染数据
		$.ajax({
            type: "get",//方法类型
            dataType: "json",//预期服务器返回的数据类型
            url: urlBase+"orderlist" ,
            data:"orderId="+orderid,
            timeout : 5000, //超时时间设置，单位毫秒
//				data:"orderId=20180514180331351325533",
            success: function(result) {
            	if(result.code == 205) {
				    mui.toast("登录过期，请重新登录！");
				    setTimeout(function() {
					  location.href="login.html";
				    }, 1000);
			    }else{
                	
					
//					基本信息
					orderInfo = result.extend.result;
					//alert(JSON.stringify(orderInfo));
					
//					顶部订单号显示
					$('#ddh').html(orderInfo.orderId);
					
					
//					保存整个详情的数据
					sessionStorage.setItem('orderInfo',JSON.stringify(orderInfo));

