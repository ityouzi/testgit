*{
	margin:0;
	padding:0;
	
}
.clear:after{
    display: block;
    content: "";
    clear: both;
    overflow: hidden;
    visibility: hidden;
}
body{
	background-color: #FFFFFF;
}
ul,li{
	list-style: none;
}
header{
	width: 100%;
	height:0.96rem;
	background-color: #000;
	text-align: center;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 99;
}

header>a>img{
	height:.4rem;
	vertical-align: middle;
	
}
header>a{
	height:100%;width:.8rem;
	position: absolute;
	left:3%;top:0;
	text-align: left;
	line-height: .96rem;
}
header>span{
	color:#fff;
	font-size:.34rem;
	line-height: .96rem;
}
.dibu-box{
	width: 100%;
	height: 1.05rem;
	background-color: #181d23;
	position: fixed;
	bottom: 0px;
	left: 0px;
	z-index: 999;
	display: flex;
	justify-content: space-around;
	align-items: center;
}
.dibu-one{
	width: 0.7rem;
	height: 0.75rem;
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	align-items: center;
}
.dibu-one img{
	height: 0.40rem;
	width: auto;
}
.dibu-one span{
	font-size: 0.22rem;
	color: #999999;
}
.leave-list{
	margin-bottom: 1.05rem;
	margin-top: 1.1rem;
	
}
.upajax{
	position: absolute;
	bottom: 0;
}
.leave-list2{
	margin-top: 1.1rem;
	margin-bottom: 2.1rem;
	position: relative;
}
.leave-one{
	width: 95%;
	margin: 0 auto;
	max-height: 3.4rem;
	border-radius: 5px;
	background-color: #F8F8F8;
	margin-top: 0.2rem;
	padding:  0.1rem 0.3rem;
	box-sizing: border-box;
	overflow: hidden;
	border:  1px solid #EEEEEE;
}
.leave-top{
	width: 100%;
	height: 0.6rem;
	display: flex;
	justify-content: space-between;
	align-items: center;
	font-size: 0.28rem;
}
.leave-time{
	width: 100%;
	height: 0.8rem;
	display: flex;
	justify-content: space-between;
	align-items: center;
	font-size: 0.28rem;
}
.leave-centen{
	width: 100%;
	height: 0.6rem;
	display: flex;
	justify-content: space-between;
	align-items: center;
	font-size: 0.28rem;
}
.leave-remakr{
	width: 100%;
	font-size: 0.28rem;
	display: -webkit-box;    
	-webkit-box-orient: vertical;    
	-webkit-line-clamp: 3;    
	overflow: hidden;
	margin-top: 0.1rem;
}
.qingjia{
	width: 2rem;
	height: 1rem;
	border-radius: 0.3rem;
	border: 1px solid #EEEEEE;
	color: red;
	position: fixed;
	left: 50%;
	bottom: 1.15rem;
	transform: translateX(-50%);
	/*background-color: #FFFFFF;*/
	text-align: center;
	line-height: 1rem;
	
}
.zhi{
	color: #999999;
}
.yl1{
	color: red;
}
.examine{
	padding: 0.1rem 0.25rem;
	background-color: #FFFFFF;
	box-shadow: 1px 1px 1px 2px #EEEEEE;
	font-size: 0.24rem;
	color: red;
	border-radius: 0.2rem;
	
	
}
.tijiao {
    box-shadow: 1px 1px 1px 2px #EEEEEE;
    position: fixed;
    bottom: 1.2rem;
    left: 50%;
    -webkit-transform: translate(-50%, 0);
	transform: translate(-50%, 0);
    width: 2rem;
    height: 0.8rem;
    border-radius: 0.35rem;
    display: flex;
    background-color: #FFFFFF;
    justify-content: center;
    align-items: center;
    font-size: 0.24rem;
    color: red;
}
.today{
	box-shadow: 1px 1px 1px 2px #EEEEEE;
    position: fixed;
    bottom: 1.2rem;
    left: 50%;
    -webkit-transform: translate(-50%, 0);
	transform: translate(-50%, 0);
    width: 2rem;
    height: 0.8rem;
    border-radius: 0.35rem;
    display: flex;
    background-color: #FFFFFF;
    justify-content: center;
    align-items: center;
    font-size: 0.24rem;
    color: red;
}
.uppage{
	box-shadow: 1px 1px 1px 2px #EEEEEE;
    position: fixed;
    bottom: 1.2rem;
    left: 10%;

    width: 1.5rem;
    height: 0.8rem;
    border-radius: 0.35rem;
    display: flex;
    background-color: #FFFFFF;
    justify-content: center;
    align-items: center;
    font-size: 0.24rem;
    color: red;
}
.downpage{
	box-shadow: 1px 1px 1px 2px #EEEEEE;
    position: fixed;
    bottom: 1.2rem;
    right: 10%;
    width: 1.5rem;
    height: 0.8rem;
    border-radius: 0.35rem;
    display: flex;
    background-color: #FFFFFF;
    justify-content: center;
    align-items: center;
    font-size: 0.24rem;
    color: red;
}
.jilubox{
	width: 95%;
	margin: 1.1rem auto 0;
	font-size:0.24rem ;
}
.jilu{
	color: red;
	font-style: normal;
}
